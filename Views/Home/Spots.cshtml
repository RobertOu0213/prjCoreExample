
@{
    ViewData["Title"] = "Spots";
}

<h1>Spots</h1>

@section Styles {
    <style>

    </style>

}
@section Scripts {
    <script>
        const searchData = {
            "categoryId": 0,
            "keyword": "北投",
            "page": 1,
            "pageSize": 9,
            "sortType": "asc",
            "sortBy": ""
        };


        (async ()=>{
                const url = "@Url.Content("~/api/spots")";
                const res = await fetch(url, {
                    method: "POST",
                    body: JSON.stringify(searchData),
                    headers: {
                        "Content-Type": "application/json"
                    }
                });
                const datas = await res.json();
                console.log(datas);

                    
                                                         
        })();

    

    </script>
}

